(u:#User:#Person *100)
(u)-[:FOLLOWS *n..n]->(u)
(product:Product {id:uuid, name:word, price:randomNumber} *200)
(order:Order {created_on: unixTime} *300)
(u)-[:CREATE_ORDER *1..n]->(order)
(order)-[:ORDER_ITEM]->(n..n)
(u)-[:COMMENTED *n..1]->(product)
(u)-[:STARRED *n..n]->(product)
(c:Category {id:uuid, name:word} *30)
(product)-[:PRODUCT_CATEGORY *n..1]->(c)
