{
    "name": "neoxygen/neogen",
    "type": "library",
    "description": "Neo4j Graph generator for PHP5.4+",
    "keywords": [
        "graph",
        "neo4j",
        "generator"
    ],
    "homepage": "http://neoxygen.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Christophe Willemsen",
            "email": "chris@neoxygen.io"
        }
    ],
    "require": {
        "php": ">=5.4",
        "fzaninotto/faker": "~1.4",
        "symfony/console": "~2.5",
        "symfony/yaml": "~2.5",
        "symfony/finder": "~2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "phpspec/phpspec": "2.1.0-RC1",
        "neoxygen/neoclient": "~1.5"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Neoxygen\\Neogen\\": "src"
        }
    }
}